generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db/development.db"
}

model User {
  id       Int      @id @default(autoincrement())
  password String
  uuid     String   @unique
  profile  Profile?
}

model Profile {
  id       Int      @id @default(autoincrement())
  user     User     @relation(fields: [user_id], references: [id])
  user_id  Int      @unique
  name     String
  age      Int
  birthday DateTime
}
